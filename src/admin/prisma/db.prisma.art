generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.0.x"]
}

datasource db {
  provider = <%= provider %>
  url      = env("DATABASE_URL")
}

model User {
  id    String     @id   {{if provider === 'mongodb'}} @map("_id") {{/if}}
  username String  @unique
  email String  @unique
  avatar String?
  password String?
  github_unionid String?
  gitee_unionid String?
}
