{
  "router": {
    "lazy": true
  },
  "publicPath": "/build/",
  "externals": {
    "moment": "moment",
    "react": "React",
    "../moment": "moment",
    "react-dom": "ReactDOM"
  },
  "plugins": [
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ],
    "./build.plugin.js"
  ],
  "proxy": {
    "/api": {
      "enable": true,
      "target": "http://0.0.0.0:9000"
    }
  }
}